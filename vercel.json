{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "buildCommand": "cd ai && npm run build",
  "outputDirectory": "ai/.next",
  "installCommand": "npm install"
} 