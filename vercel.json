{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "ai/.next",
  "installCommand": "npm install",
  "trailingSlash": false
} 